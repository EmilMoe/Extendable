{
  "name": "EmilMoe/Extendable",
  "description": "Allowing methods classes to be extended with static and non-static methods. Also supporting classes with already magic methods.",
  "homepage": "https://emilmoe.com",
  "license": "MIT",
  "keywords": [
    "Runtime extension"
  ],
  "support": {
    "issues": "https://github.com/emilmoe/extendable/issues",
    "source": "https://github.com/emilmoe/extendable"
  },
  "authors": [
    {
      "name": "Emil Moe",
      "email": "emil@moegroup.dk"
    }
  ],
  "require": {
    "illuminate/support": "~5.1",
    "php" : "~5.6|~7.0"
  },
  "require-dev": {
    "phpunit/phpunit" : "~4.0||~5.0",
    "scrutinizer/ocular": "~1.1",
    "squizlabs/php_codesniffer": "~2.3"
  },
  "autoload": {
    "psr-4": {
      "EmilMoe\\Extendable\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "EmilMoe\\Extendable\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
